  },
  "homepage": "https://github.com/ridrum/puppeteer-cucumber#readme",
  "dependencies": {
    "cucumber": "^3.0.0",
    "puppeteer": "0.10.0"
  },
  "devDependencies": {
    "eslint": "4.5.0",
    "eslint-config-airbnb-base": "11.3.2",
    "eslint-config-prettier": "2.3.0",
    "eslint-plugin-import": "2.7.0",
    "prettier": "1.5.3"
  }
